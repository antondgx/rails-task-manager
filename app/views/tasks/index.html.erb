<h1>Show all Tasks</h1>

<%= link_to "CREATE NEW TASK", tasks_new_path %>
<br>
<br>
<% @tasks.each do |task| %>
<h4> [<%= task.completed %>] <%= task.title %> </h4>
<%= link_to "Edit", edit_task_path(task) %>
<%= link_to "Delete", task_path(task), method: :delete, data: { confirm: "Are you sure?" } %>
<br>
<br>
<% end %>
